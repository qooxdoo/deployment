{
  "$schema": "https://qooxdoo.org/schema/compile-1-0-0.json",
  "environment": {
    "qx.serve.appspath": "/apps/",
    "qx.serve.docspath": "/"
  },
  "targets": [
    {
      "type": "build",
      "outputPath": "compiled/build"
    }
  ],
  "defaultTarget": "build",
  "locales": [
    "en"
  ],
  "ignores": [
    "module.*",
    "process.*",
    "Promise.*",
    "require.*",
    "path.*",
    "args.*"
  ],
  "libraries": [
    "./qooxdoo/framework"
  ],
  "applications": [
    {
      "class": "qxl.apiviewer.Application",
      "theme": "qxl.apiviewer.Theme",
      "name": "apiviewer",
      "title": "Qooxdoo compiler API",
      "default": true,
      "environment": {
        "qx.icontheme": "Tango",
        "excludeFromAPIViewer": [
          "qxl.*",
          "qxWeb"
        ]
      },
      "include": [
        "qx.*",
        "qxl.*"
      ],
      "exclude": [
        "qx.ui.website.*",
        "*.index"
      ]
    }
  ]
}
